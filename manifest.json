{
  "manifest_version": 3,
  "name": "Skip Wait",
  "version": "1.1",
  "description": "Skip countdown timers and waiting pages. One-click bypass for link shorteners and file hosts.",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
